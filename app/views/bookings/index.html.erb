<div class ='container'>

  <h1>My bookings</h1>


  <% if @bookings != [] %>


    <% @bookings.each do |booking| %>
      <div class="card">
        <%= booking.comment %>
        <%= booking.start_date %>
        <%= booking.end_date %>
        <%= booking.status %>
    <% end %>
    </div>

  <% else %>
    <p>You don't have booking</p>

  <% end %>

</div>
